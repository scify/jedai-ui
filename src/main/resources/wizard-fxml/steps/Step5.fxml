<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
      minWidth="-Infinity" spacing="5.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scify.jedai.gui.controllers.steps.Step5Controller">
    <children>
        <Label text="Entity Matching">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <Label text="Select parameters for Entity Matching">
            <font>
                <Font size="14.0"/>
            </font>
        </Label>
        <HBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="680.0" spacing="10.0">
            <children>
                <VBox fx:id="matchingMethodContainer" prefHeight="200.0" prefWidth="340.0" spacing="5.0">
                    <children>
                        <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Entity Matching Method"
                               underline="true"/>
                    </children>
                </VBox>
                <VBox fx:id="confTypeContainer" prefHeight="200.0" prefWidth="340.0">
                    <children>
                        <Label text="Configuration type:">
                            <font>
                                <Font name="System Bold" size="12.0"/>
                            </font>
                        </Label>
                    </children>
                </VBox>
            </children>
            <VBox.margin>
                <Insets left="20.0" right="20.0"/>
            </VBox.margin>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" top="10.0"/>
    </padding>
</VBox>
